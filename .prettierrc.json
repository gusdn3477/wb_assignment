{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^react-router-dom$",
    "^react(.*)$",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "^@/store/(.*)$",
    "^@/api/(.*)$",
    "^@/hooks/(.*)$",
    "^@/util(.*)$",
    "^@/utils/(.*)$",
    "^@/util/(.*)$",
    "^@/constants",
    "^@/constants/(.*)$",
    "^@/pages/(.*)$",
    "^@/services/(.*)$",
    "^@/components/(.*)$",
    "^@/assets/(.*)$",
    "^[./]"
  ]
}
